{
  "$id": "http://schema.asemicanalytics.com/v1/semantic_layer/entity_property_event.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "source_event",
    "select",
    "aggregate_function"
  ],
  "properties": {
    "source_event": {
      "type": "string"
    },
    "select": {
      "type": "string"
    },
    "aggregate_function": {
      "type": "string",
      "enum": [
        "count",
        "sum",
        "avg",
        "min",
        "max",
        "first_value",
        "last_value",
        "none"
      ]
    },
    "where": {
      "type": "string"
    },
    "default_value": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
